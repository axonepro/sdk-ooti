import ooti.ooti as oo



def fetch_team_members(email:str,mdp:str,team_pk:str):
    '''
        Display members of a team
    '''
    my_account = oo.OotiAPI(email,mdp)
    my_account.connect()
    team_members = my_account.Teams.get_team_users_list(team_pk)
    print(f"Team {team_pk} members':\n")
    for member in team_members["data"]:
        print(f"{member['display_name']} - {member['role_display']}")
